{
	"name": "debounce",
	"version": "2.1.0",
	"description": "Delay function calls until a set time elapses after the last invocation",
	"license": "MIT",
	"repository": "sindresorhus/debounce",
	"funding": "https://github.com/sponsors/sindresorhus",
	"exports": {
