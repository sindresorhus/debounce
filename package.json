		"rate",
		"batch",
		"ratelimit"
	],
	"devDependencies": {
		"sinon": "^17.0.1",
		"xo": "^0.56.0"
	},
	"xo": {
		"rules": {
			"unicorn/prefer-module": "off"
		}
	}
}
